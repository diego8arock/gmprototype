; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="gmprototype"
run/main_scene="res://game/Game.tscn"
config/icon="res://icon.png"

[autoload]

GlobalConstant="*res://utils/autoload/global_constant.gd"
GlobalVariable="*res://utils/autoload/global_variable.gd"
LevelLoader="*res://utils/autoload/level_loader.gd"

[display]

window/size/width=360
window/size/height=640
window/size/test_width=360
window/size/test_height=640

[input]

fire=[ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":32,"unicode":0,"echo":false,"script":null)
 ]

[layer_names]

2d_physics/layer_1="player_hitbox"
2d_physics/layer_2="player_weapon"
2d_physics/layer_3="enemy_hitbox"
2d_physics/layer_4="enemy_weapon"

[rendering]

environment/default_environment="res://default_env.tres"
